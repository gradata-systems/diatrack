{
  "priority": 1,
  "index_patterns": [
    "diatrack_v1*_account_state"
  ],
  "template": {
    "settings": {
      "number_of_replicas": 2,
      "number_of_shards": 1,
      "refresh_interval": "1s"
    },
    "mappings": {
      "dynamic": false,
      "_source": {
        "enabled": true
      },
      "_routing": {
        "required": false
      },
      "properties": {
        "pollingEnabled": {
          "type": "boolean"
        },
        "lastReceived": {
          "type": "date"
        }
      }
    }
  }
}