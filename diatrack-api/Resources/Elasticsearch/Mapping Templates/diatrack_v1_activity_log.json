{
  "priority": 1,
  "index_patterns": [
    "diatrack_v1*_activity_log"
  ],
  "template": {
    "settings": {
      "number_of_replicas": 2,
      "number_of_shards": 1,
      "refresh_interval": "1s"
    },
    "mappings": {
      "dynamic": true,
      "_source": {
        "enabled": true
      },
      "_routing": {
        "required": false
      },
      "properties": {
        "@created": {
          "type": "date"
        },
        "createdBy": {
          "properties": {
            "id": {
              "type": "keyword"
            },
            "name": {
              "type": "keyword"
            },
            "emailAddress": {
              "type": "keyword"
            }
          }
        },
        "accountId": {
          "type": "keyword"
        },
        "category": {
          "type": "keyword"
        },
        "bgl": {
          "type": "float"
        },
        "properties": {
          "type": "object"
        },
        "notes": {
          "type": "text"
        }
      }
    }
  }
}